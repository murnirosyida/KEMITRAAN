        <div id="loading" class="loading">
            <div class="spinner"></div>
            <p>Sedang generate address...</p>
        </div>
    </div>

    <script>
        // Cek apakah libraries terload dengan benar
        let librariesLoaded = false;
        
        // Fungsi untuk inisialisasi
        function init() {
            if (typeof elliptic !== '\''undefined'\'' && typeof sha3_256 !== '\''undefined'\'') {
                librariesLoaded = true;
                addLog('\''‚úÖ Libraries loaded: elliptic.js + js-sha3'\'', '\''success'\'');
                addLog('\''üîë Private Key ‚Üí Ethereum Address (0x1...)'\'', '\''info'\'');
                addLog('\''Siap untuk konversi massal'\'', '\''info'\'');
            } else {
                showError('\''Libraries tidak terload. Pastikan koneksi internet aktif.'\'');
                librariesLoaded = false;
            }
        }

        // Tunggu DOM siap sebelum inisialisasi
        if (document.readyState === '\''loading'\'') {
            document.addEventListener('\''DOMContentLoaded'\'', init);
        } else {
            init();
        }

        // Gunakan elliptic library untuk ECDSA yang proper
        const ec = librariesLoaded ? new elliptic.ec('\''secp256k1'\'') : null;

        // Fungsi untuk validasi private key="********" validatePrivateKey(privateKeyHex) {
            try {
                let cleanPrivateKey="priv********yHex".replace(/^0x/, '\'''\'');
                
                // Cek panjang
                if (cleanPrivateKey.length < 64) {
                    return { valid: false, message: '\''Private key terlalu pendek'\'' };
                }
                if (cleanPrivateKey.length > 64) {
                    return { valid: false, message: '\''Private key terlalu panjang'\'' };
                }
                
                // Cek format hex
                if (!/^[0-9a-fA-F]+$/.test(cleanPrivateKey)) {
                    return { valid: false, message: '\''Format hex tidak valid'\'' };
                }
                
                // Cek apakah private key valid di kurva secp256k1
                if (librariesLoaded) {
                    try {
                        ec.keyFro="********"(cleanPrivateKey, '\''hex'\'');
                    } catch (e) {
                        return { valid: false, message: '\''Private key tidak valid untuk kurva secp256k1'\'' };
                    }
                }
                
                return { valid: true, message: '\''Private key valid'\'' };
            } catch (error) {
                return { valid: false, message: '\''Error validasi: '\'' + error.message };
            }
        }

        // Fungsi untuk generate address dari private key="********" privateKeyT="********"(privateKeyHex) {
            if (!librariesLoaded) {
                throw new Error('\''Libraries tidak tersedia'\'');
            }
            
            try {
                // Normalisasi private key
                let cleanPrivateKey="priv********yHex".replace(/^0x/, '\'''\'');
                if (cleanPrivateKey.length < 64) {
                    cleanPrivateKey = '\''0'\''.repeat(64 - cleanPrivateKey.length) + cleanPrivateKey;
                }
                if (cleanPrivateKey.length !== 64) {
                    throw new Error('\''Private key harus 64 karakter hex'\'');
                }

                // Validasi hex
                if (!/^[0-9a-fA-F]+$/.test(cleanPrivateKey)) {
                    throw new Error('\''Format hex tidak valid'\'');
                }

                // Buat key pair dari private key
                const keyPair = ec.keyFro="********"(cleanPrivateKey, '\''hex'\'');
                
                // Dapatkan public key dalam format uncompressed (04 prefix)
                const publicKey = keyPair.getPublic(false, '\''hex'\'');
                
                // Public key harus diawali dengan '\''04'\'' dan panjang 130 karakter
                if (!publicKey.startsWith('\''04'\'') || publicKey.length !== 130) {
                    throw new Error('\''Public key format invalid: '\'' + publicKey);
                }

                // Ambil bagian X dan Y dari public key (tanpa prefix 04)
                const publicKeyBytes="publ********cKey".substring(2);
                
                // Convert hex string to array of numbers untuk hashing
                const publicKeyArray = [];
                for (let i = 0; i < publicKeyBytes.length; i += 2) {
                    publicKeyArray.push(parseInt(publicKeyBytes.substr(i, 2), 16));
                }

                // Hash dengan Keccak-256
                const hash = sha3_256(publicKeyArray);
                
                // Ambil 20 bytes terakhir (40 karakter) untuk address
                const address = '\''0x'\'' + hash.substring(hash.length - 40);
                
                // Validasi address harus mulai dengan 0x1 dan panjang 42
                if (!address.startsWith('\''0x1'\'') || address.length !== 42) {
                    throw new Error('\''Generated address invalid: '\'' + address);
                }

                return address;
            } catch (error) {
                throw new Error('\''Conversion failed: '\'' + error.message);
            }
        }

        // Fungsi untuk checksum address (EIP-55)
        function toChecksumAddress(address) {
            address = address.toLowerCase().replace('\''0x'\'', '\'''\'');
            const hash = sha3_256(address);
            let checksumAddress = '\''0x'\'';
            
            for (let i = 0; i < address.length; i++) {
                if (parseInt(hash[i], 16) >= 8) {
                    checksumAddress += address[i].toUpperCase();
                } else {
                    checksumAddress += address[i];
                }
            }
            
            return checksumAddress;
        }

        // Fungsi UI
        function showError(message) {
            const errorDiv = document.getElementById('\''errorMessage'\'');
            errorDiv.textContent = '\''‚ùå '\'' + message;
            errorDiv.style.display = '\''block'\'';
            hideOtherMessages('\''error'\'');
        }

        function showSuccess(message) {
            const successDiv = document.getElementById('\''successMessage'\'');
            successDiv.textContent = '\''‚úÖ '\'' + message;
            successDiv.style.display = '\''block'\'';
            hideOtherMessages('\''success'\'');
        }

        function showWarning(message) {
            const warningDiv = document.getElementById('\''warningMessage'\'');
            warningDiv.textContent = '\''‚ö†Ô∏è '\'' + message;
            warningDiv.style.display = '\''block'\'';
            hideOtherMessages('\''warning'\'');
        }

        function hideOtherMessages(except) {
            const messages = ['\''error'\'', '\''success'\'', '\''warning'\''];
            messages.forEach(type => {
                if (type !== except) {
                    document.getElementById(type + '\''Message'\'').style.display = '\''none'\'';
                }
            });
        }

        function hideAllMessages() {
            hideOtherMessages('\'''\'');
        }

        function addLog(message, type = '\''info'\'') {
            const logContainer = document.getElementById('\''logContainer'\'');
            const logEntry = document.createElement('\''div'\'');
            logEntry.className = `log-entry log-${type}`;
            
            const timestamp = new Date().toLocaleTimeString();
            logEntry.innerHTML = `[${timestamp}] ${message}`;
            
            logContainer.appendChild(logEntry);
            logContainer.style.display = '\''block'\'';
            logContainer.scrollTop = logContainer.scrollHeight;
        }

        function clearLog() {
            const logContainer = document.getElementById('\''logContainer'\'');
            logContainer.innerHTML = '\'''\'';
            logContainer.style.display = '\''none'\'';
        }

        function showLoading() {
            document.getElementById('\''loading'\'').style.display = '\''block'\'';
        }

        function hideLoading() {
            document.getElementById('\''loading'\'').style.display = '\''none'\'';
        }

        function updateProgress(percent) {
            const progressContainer = document.getElementById('\''progressContainer'\'');
            const progressBar = document.getElementById('\''progressBar'\'');
            
            if (percent > 0) {
                progressContainer.style.display = '\''block'\'';
                progressBar.style.width = `${percent}%`;
            } else {
                progressContainer.style.display = '\''none'\'';
                progressBar.style.width = '\''0%'\'';
            }
        }

        function updateStats(processed, success, error, time) {
            document.getElementById('\''totalProcessed'\'').textContent = processed;
            document.getElementById('\''successCount'\'').textContent = success;
            document.getElementById('\''errorCount'\'').textContent = error;
            document.getElementById('\''processingTime'\'').textContent = time + '\''s'\'';
        }

        function convertToAddress() {
            if (!librariesLoaded) {
                showError('\''Libraries tidak tersedia. Pastikan koneksi internet aktif.'\'');
                return;
            }
            
            const inputText = document.getElementById('\''inputHex'\'').value.trim();
            
            if (!inputText) {
                showError('\''Silakan masukkan private key hex terlebih dahulu!'\'');
                return;
            }

            showLoading();
            clearLog();
            hideAllMessages();
            updateProgress(0);

            const startTime = performance.now();

            let hexArray = inputText.split('\''\n'\'')
                .map(item => item.trim())
                .filter(item => item);

            // Validasi awal semua private key
            let invalidKeys = [];
            hexArray.forEach((hex, index) => {
                const validation = validatePrivateKey(hex);
                if (!validation.valid) {
                    invalidKeys.push({ index: index + 1, key: hex, reason: validation.message });
                }
            });

            if (invalidKeys.length > 0) {
                let errorMsg = `Ditemukan ${invalidKeys.length} private key tidak valid:\n`;
                invalidKeys.forEach(item => {
                    errorMsg += `Baris ${item.index}: ${item.key} - ${item.reason}\n`;
                });
                
                showError(errorMsg);
                hideLoading();
                updateProgress(0);
                return;
            }

            let successCount = 0;
            let errorCount = 0;
            const results = [];

            addLog(`Memulai proses ${hexArray.length} private key...`, '\''info'\'');
            addLog('\''Menggunakan elliptic.js untuk ECDSA secp256k1'\'', '\''info'\'');

            // Proses batch dengan progress update
            hexArray.forEach((hex, index) => {
                try {
                    // Update progress
                    const progress = ((index + 1) / hexArray.length) * 100;
                    updateProgress(progress);
                    
                    // Generate address dari private key
                    const address = privateKeyT="********"(hex);
                    const checksumAddress = toChecksumAddress(address);
                    
                    results.push({
                        input: hex,
                        address: checksumAddress,
                        status: '\''success'\''
                    });
                    
                    successCount++;
                    addLog(`‚úì ${hex.substring(0, 16)}... -> ${checksumAddress}`, '\''success'\'');

                } catch (error) {
                    results.push({
                        input: hex,
                        address: `ERROR: ${error.message}`,
                        status: '\''error'\''
                    });
                    
                    errorCount++;
                    addLog(`‚úó ${hex.substring(0, 16)}... -> ERROR: ${error.message}`, '\''error'\'');
                }
            });

            // Format output
            const outputText = results.map(item => {
                return `${item.input} -> ${item.address}`;
            }).join('\''\n'\'');

            document.getElementById('\''outputAddress'\'').value = outputText;
            
            const endTime = performance.now();
            const processingTime = ((endTime - startTime) / 1000).toFixed(2);

            updateStats(hexArray.length, successCount, errorCount, processingTime);
            hideLoading();
            updateProgress(0);

            if (errorCount === 0 && successCount > 0) {
                showSuccess(`Berhasil generate ${successCount} addresses valid!`);
            } else if (errorCount > 0 && successCount > 0) {
                showWarning(`${successCount} berhasil, ${errorCount} gagal`);
            } else if (errorCount === hexArray.length) {
                showError('\''Semua konversi gagal. Periksa format private key.'\'');
            }

            addLog(`Proses selesai: ${successCount} sukses, ${errorCount} error, waktu: ${processingTime}s`, '\''info'\'');
        }

        function clearAll() {
            document.getElementById('\''inputHex'\'').value = '\'''\'';
            document.getElementById('\''outputAddress'\'').value = '\'''\'';
            updateStats(0, 0, 0, 0);
            clearLog();
            hideAllMessages();
            updateProgress(0);
        }

        function downloadResults() {
            const outputText = document.getElementById('\''outputAddress'\'').value;
            
            if (!outputText) {
                showError('\''Tidak ada data untuk diunduh!'\'');
                return;
            }

            try {
                const timestamp = new Date().toISOString().slice(0,19).replace(/:/g,'\''-'\'');
                const blob = new Blob([outputText], { type: '\''text/plain;charset=utf-8'\'' });
                const url = URL.createObjectURL(blob);
                const a = document.createElement('\''a'\'');
                a.href = url;
                a.download = `ethereum_addresses_${timestamp}.txt`;
                document.body.appendChild(a);
                a.click();
                document.body.removeChild(a);
                URL.revokeObjectURL(url);
                
                showSuccess('\''File berhasil diunduh!'\'');
            } catch (error) {
                showError('\''Gagal mengunduh file: '\'' + error.message);
            }
        }

        function loadTestData() {
            const testData = [
                '\''0000********************************************************57DD'\'',
                '\''4f3a***********************************************************a2b1'\'',
                '\''a1b2**********************************************************7890'\''
            ];
            
            document.getElementById('\''inputHex'\'').value = testData.join('\''\n'\'');
            showSuccess('\''Data test dimuat. Klik "Generate Address"!'\'');
            addLog('\''Data test dimuat - menggunakan elliptic.js library'\'', '\''success'\'');
        }

        // Event listener untuk validasi real-time
        document.getElementById('\''inputHex'\'').addEventListener('\''input'\'', function() {
            const inputText = this.value.trim();
            const validationDiv = document.getElementById('\''inputValidation'\'');
            
            if (!inputText) {
                validationDiv.style.display = '\''none'\'';
                return;
            }
            
            const lines = inputText.split('\''\n'\'')
                .map(item => item.trim())
                .filter(item => item);
            
            if (lines.length === 0) {
                validationDiv.style.display = '\''none'\'';
                return;
            }
            
            let validCount = 0;
            let invalidCount = 0;
            
            lines.forEach(line => {
                const validation = validatePrivateKey(line);
                if (validation.valid) {
                    validCount++;
                } else {
                    invalidCount++;
                }
            });
            
            if (invalidCount === 0) {
                validationDiv.textContent = `‚úÖ Semua ${validCount} private key valid`;
                validationDiv.className = '\''input-validation input-valid'\'';
            } else if (validCount === 0) {
                validationDiv.textContent = `‚ùå Semua ${invalidCount} private key tidak valid`;
                validationDiv.className = '\''input-validation input-invalid'\'';
            } else {
                validationDiv.textContent = `‚ö†Ô∏è ${validCount} valid, ${invalidCount} tidak valid`;
                validationDiv.className = '\''input-validation input-warning'\'';
            }
            
            validationDiv.style.display = '\''block'\'';
        });

        document.getElementById('\''inputHex'\'').addEventListener('\''keydown'\'', function(e) {
            if (e.ctrlKey && e.key === '\''Enter'\'') {
                convertToAddress();
            }
        });
    </script>
